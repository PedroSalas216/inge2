MEETING = (hello -> converse -> goodbye -> STOP).
GAME 	= 	(tree -> lose -> GAME 
	 		|{one, two} -> win -> GAME).
JOB 	= (arrive -> work -> leave -> JOB).
MOVIE	= (ahead -> (right -> MOVIE 
					|left -> STOP)).
FOURTICK= F[0],
F[i:0..4] = (when i<4  tick -> F[i+1]
			|when i==4 tick -> STOP).

DOUBLE = (in [i:1..3] -> out[i] -> DOUBLE).

PERSON = (weekend -> SUNDAY | weekend -> MONDAY),
SUNDAY = (sleep -> {play, shop} -> PERSON),
MONDAY = (sleep -> work -> PERSON).




